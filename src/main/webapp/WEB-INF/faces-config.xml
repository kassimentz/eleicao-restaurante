<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">

	<application>
		<locale-config>
			<default-locale>pt_BR</default-locale>
		</locale-config>
	</application>

	<converter>
		<display-name>RestauranteConverter</display-name>
		<converter-id>restauranteConverter</converter-id>
		<converter-class>com.teste.eleicaoRestaurante.converter.RestauranteConverter</converter-class>
	</converter>
	
	<converter>
		<display-name>CpfConverter</display-name>
		<converter-id>cpfConverter</converter-id>
		<converter-class>com.teste.eleicaoRestaurante.converter.CpfConverter</converter-class>
	</converter>
</faces-config>
